
#server 0 /vagrant/code/tc_helper.sh log_buffer_size
client0 0 iperf -s &>/vagrant/log/iperf_s.log & echo $! > /tmp/pid_iperf_server
server 0 truncate -s0 /vagrant/log/iperf_c.log

server 0 /vagrant/code/tc_helper.sh set_bw 300kbit
server 0 sudo rmmod tcp_probe ; sudo modprobe tcp_probe port=5001 full=1
server 2 sudo cat /proc/net/tcpprobe &>/vagrant/log/tcp_probe_300kbit.log & echo $! > /tmp/pid_tcpprobe_cat
server 2 sudo tcpdump -i eth1 -s0 -w /vagrant/log/tcp_probe_300kbit.pcap 2>&1 & echo $! > /tmp/pid_tcpdump
server 5 iperf -c client0 -t 300 >>/vagrant/log/iperf_c.log
server 327 sudo kill -SIGTERM $(cat /tmp/pid_tcpprobe_cat) & sudo kill -SIGTERM $(cat /tmp/pid_tcpdump)

